{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test"
        ],
        "accessToken": "YzM0ZmE5ZDMtZjhjYS00ZmQ3LThlZjUtYjEyYWJhNmZkYWM2fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "all": [
      "{projectRoot}/**/*"
    ],
    "public": [
      "!{projectRoot}/**/*.test.*"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "public",
        "^public"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "all",
        "^public"
      ]
    }
  },
  "defaultBase": "master"
}